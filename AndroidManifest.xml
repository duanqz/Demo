<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.xo.demo" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-feature android:name="android.hardware.camera" />

    <uses-sdk android:minSdkVersion="14" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Sample code activities -->
        <!-- ================ Widget ================ -->
        <activity
            android:name=".widget.tabmenu.TabMenuActivity" 
            android:label="@string/label_tab_menu"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".widget.rotation3d.Rotation3dActivity" 
            android:label="@string/label_rotation3d_button"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".widget.viewflipper.ViewFlipperActivity" 
            android:label="@string/label_viewflipper"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <!-- ================ Sensor ================ -->
        <activity
            android:name=".sensor.flashlight.FlashLightActivity" 
            android:label="@string/label_flashlight"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity
            android:name=".sensor.location.TestLocationActivity" 
            android:label="@string/label_location"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <!-- ================ Hello World ================ -->
        <activity
            android:name=".helloworld.helloservice.HelloServiceActivity" 
            android:label="@string/label_aidl"
            android:exported="false" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <service android:name=".helloworld.helloservice.local.LocalService"
            android:exported="false" > 
            <intent-filter> 
                <action android:name="org.xo.demo.helloworld.helloservice.local.LocalService" /> 
            </intent-filter>  
        </service>

        <service android:name=".helloworld.helloservice.remote.RemoteService"
            android:process=":remote"
            android:exported="false" > 
            <intent-filter> 
                <action android:name="org.xo.demo.helloworld.helloservice.remote.RemoteService" /> 
            </intent-filter>  
        </service>
    </application>

</manifest>